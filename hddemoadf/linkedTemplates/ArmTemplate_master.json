{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hddemoadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"linkedService1_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'linkedService1'"
		},
		"linkedService1_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'linkedService1'"
		},
		"linkedService1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'linkedService1'"
		},
		"ls_str_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_str'"
		},
		"linkedService1_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "804be721-2d79-4132-a058-b0560eceb187"
		},
		"linkedService1_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "newlabhddemo"
		},
		"linkedService1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "cb0207aa-069d-4cf6-bda1-a430871702e1"
		},
		"linkedService1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hddemoadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"ls_str_connectionString": {
						"value": "[parameters('ls_str_connectionString')]"
					},
					"linkedService1_clusterPassword": {
						"value": "[parameters('linkedService1_clusterPassword')]"
					},
					"linkedService1_clusterSshPassword": {
						"value": "[parameters('linkedService1_clusterSshPassword')]"
					},
					"linkedService1_servicePrincipalKey": {
						"value": "[parameters('linkedService1_servicePrincipalKey')]"
					},
					"linkedService1_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('linkedService1_properties_typeProperties_hostSubscriptionId')]"
					},
					"linkedService1_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('linkedService1_properties_typeProperties_clusterResourceGroup')]"
					},
					"linkedService1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('linkedService1_properties_typeProperties_servicePrincipalId')]"
					},
					"linkedService1_properties_typeProperties_tenant": {
						"value": "[parameters('linkedService1_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}